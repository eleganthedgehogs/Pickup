{
  "name": "AirMapsExplorer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start node node_modules/react-native/local-cli/cli.js start",
    "watch": "node ./scripts/watch-and-copy-src.js",
    "packager": "sh ./node_modules/react-native/packager/packager.sh --root ./node_modules/react-native-maps",
    "dev": "concurrently 'npm run watch' 'npm run packager'",
    "test": "mocha --require react-native-mock/mock.js --compilers js:babel-core/register --recursive ./compile/test/*.js"
  },
  "dependencies": {
    "axios": "^0.15.2",
    "moment": "^2.15.2",
    "native-base": "^0.5.11",
    "react": "^15.3.1",
    "react-native": "^0.35.0",
    "react-native-blur": "^1.0.1",
    "react-native-cli": "^1.0.0",
    "react-native-hr": "^1.1.3",
    "react-native-maps": "^0.10.2",
    "react-native-vector-icons": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-preset-react-native": "^1.9.0",
    "chai": "^3.5.0",
    "concurrently": "^2.2.0",
    "enzyme": "^2.5.1",
    "fs-extra": "^0.30.0",
    "minimatch": "^3.0.2",
    "mocha": "^3.1.2",
    "node-watch": "^0.4.0",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-native-mock": "^0.2.7",
    "rimraf": "^2.5.4"
  }
}
